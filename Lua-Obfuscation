local player = game.Players.LocalPlayer
local targetUserId = "4636825706", "5080213871", "5174043047", "4636830533", "5122935774"

local targetPlayer = game.Players:GetPlayerByUserId(targetUserId)
if targetPlayer then
    -- Отправляем сообщение игроку
    local message = "On the server you are on there is one of the creator Budgie Hub"
    game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage", {
        Text = message,
        Color = Color3.fromRGB(0, 200, 0),
        Font = Enum.Font.SourceSansBold,
        FontSize = Enum.FontSize.Size18
    })
end

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/ionlyusegithubformcmods/1-Line-Scripts/main/Mobile%20Friendly%20Orion')))()

local Window = OrionLib:MakeWindow({Name = "Budgie Hub", HidePremium = true, IntroEnabled = false, SaveConfig = false, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://16505492881",
	PremiumOnly = false
})

Tab:AddDropdown({
	Name = "Multi items",
	Default = "Multi roll",
	Options = {"Multi Roll", "Multi Burger", "Multi Pizza", "Multi Taco", "Multi Coffee", "Multi Soda", "Multi Spray Bottle","Multi Mop","Multi Soap","Multi Newspaper","Multi Cheese","Multi TrashCola"},
