Tab:AddDropdown({
	Name = "Part Control v2",
	Default = "",
	Options = {"Part Anchored", "Part Unanchored"},
	Callback = function(Value)
		if Value == "Part Anchored" then
		for _, part in ipairs(workspace:GetDescendants()) do
        if part:IsA("BasePart") then
            part.Anchored = true
        end
    end
		end
		if Value == "Part Unanchored" then
		for _, part in ipairs(workspace:GetDescendants()) do
        if part:IsA("BasePart") then
            part.Anchored = fasle
        end
    end
		end
	end    
})

Tab:AddDropdown({
	Name = "Btools",
	Default = "1",
	Options = {"Delete tool", "Clone tool", "Grab tool", "F3X", "Black hole tool", "White hole tool", "Snatch tool", "Gravity hunchback", "Push tool", "Explosion tool", "Neo Focus"},
	Callback = function(Value)
		if Value == "Delete tool" then
		backpack = game:GetService("Players").LocalPlayer.Backpack

hammer = Instance.new("HopperBin")
hammer.Name = "Hammer"
hammer.BinType = 4
hammer.Parent = backpack
		end
		if Value == "Clone tool" then
		backpack = game:GetService("Players").LocalPlayer.Backpack
		
		cloneTool = Instance.new("HopperBin")
cloneTool.Name = "Clone"
cloneTool.BinType = 3
cloneTool.Parent = backpack
		end
		if Value == "Grab tool" then
		backpack = game:GetService("Players").LocalPlayer.Backpack
		
		grabTool = Instance.new("HopperBin")
grabTool.Name = "Grab"
grabTool.BinType = 2
grabTool.Parent = backpack
if Value == "F3X" then
loadstring(game:GetObjects("rbxassetid://6695644299")[1].Source)()
		end
end
		if Value == "Black hole tool" then
		local tool = Instance.new("Tool")
tool.RequiresHandle = false -- Исправлена опечатка здесь
tool.Name = "Black hole tool"
tool.TextureId = "http://www.roblox.com/asset/?id=16466291679"

local isEnabled = false

local function onActivated()
    isEnabled = not isEnabled

    if not isEnabled then return end

    local plr = game.Players.LocalPlayer
    local char = plr.Character
    local SwEF = 1e9999 * 1e9999 * 1e9999

    for _, child in pairs(char:GetChildren()) do
        if child:IsA("BasePart") then
            child.Massless = false
            child.CustomPhysicalProperties = PhysicalProperties.new(SwEF, SwEF, SwEF)
        end
    end

    local player = game.Players.LocalPlayer
    local mouse = player:GetMouse()
    local clickPosition = mouse.Hit.p

    while isEnabled and wait() do
        for _, item in pairs(workspace:GetDescendants()) do
            if item:IsA("Part") or item:IsA("MeshPart") then
                if item.Anchored == false then
                    item:ApplyImpulse((clickPosition - item.Position).unit * 1500)
                end
            end
        end
    end
end

tool.Activated:Connect(onActivated)

tool.Parent = game.Players.LocalPlayer.Backpack
		end
		if Value == "White hole tool" then
		local plr = game.Players.LocalPlayer
local char = plr.Character
SwEF = 1e9999 * 1e9999 * 1e99

for _, child in pairs(char:GetChildren()) do
    if child:IsA("BasePart") then
        child.Massless = false
        child.CustomPhysicalProperties = PhysicalProperties.new(SwEF, SwEF, SwEF) 
    end
end

local tool = Instance.new("Tool")
tool.Name = "White hole tool"
tool.RequiresHandle = false
tool.TextureId = "http://www.roblox.com/asset/?id=16466455509"

local toggle = true -- Create a toggle variable

tool.Activated:Connect(function()
    if toggle then
        local mouse = plr:GetMouse() -- Fix error by replacing player with plr
        local clickPosition = mouse.Hit.p

        local radius = 10

        for _, item in pairs(workspace:GetDescendants()) do
            if (item:IsA("Part") or item:IsA("MeshPart")) and not item.Anchored then
                local distance = (item.Position - clickPosition).magnitude

                if distance <= radius then
                    local direction = (item.Position - clickPosition).unit
                    item:ApplyImpulse(direction * 10000)
                end
            end
        end
    end
    toggle = not toggle -- Toggle the variable
end)

tool.Parent = game.Players.LocalPlayer.Backpack
		end
if Value == "Snatch tool" then
local TweenService = game:GetService("TweenService")
local tool = Instance.new("Tool")
tool.Name = "Snatch Tool"
tool.RequiresHandle = false
local distance = 40
local cooldown = 1 -- Кулдаун в секундах, чтобы избежать спама

local isCooldown = false

tool.Activated:Connect(function()
    if isCooldown then return end
    isCooldown = true
    
    local character = tool.Parent
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    if humanoidRootPart then
        -- Вычисляем новую позицию
        local newCFrame = humanoidRootPart.CFrame * CFrame.new(0, 0, -distance)
        
        -- Создаем твин
        local tweenInfo = TweenInfo.new(
            0.2, -- Длительность анимации в секундах
            Enum.EasingStyle.Linear, -- Стиль изменения (линейный делает движение равномерным)
            Enum.EasingDirection.Out, -- Направление изменения
            0, -- Количество повторений (0 означает без повторений)
            false, -- Необходимость обратного движения
            0 -- Задержка перед началом анимации
        )
        local tween = TweenService:Create(humanoidRootPart, tweenInfo, {CFrame = newCFrame})
        
        tween:Play() -- Начинаем анимацию
        tween.Completed:Wait() -- Ждем окончания анимации
        
        -- Отталкиваем неприкрепленные объекты
        local parts = workspace:GetDescendants()
        for _, part in ipairs(parts) do
            if part:IsA("BasePart") and not part:IsDescendantOf(character) and part.Anchored == false then
                local direction = (part.Position - humanoidRootPart.Position).Unit
                local force = direction * 1000 -- Можно изменить силу отталкивания здесь
                part.Velocity = force
            end
        end
    end
    
    -- Устанавливаем кулдаун
    wait(cooldown)
    isCooldown = false
end)

tool.Parent = game.Players.LocalPlayer.Backpack
end
