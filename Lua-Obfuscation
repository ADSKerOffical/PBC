    if tool:IsA("Tool") and tool.Name == "TpRoll" then
      tool.GripPos = Vector3.new(math.random(-20, 20), 0, math.random(-20, 20))
    end
  end
  wait() 
end
        end
        if Value == "Burger" then
while wait() do
