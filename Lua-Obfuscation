StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, true)
end
      if Value == "Enable emotes" then
      game.StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.EmotesMenu, false)
      end
      if Value == "Enable leaderboard" then
