Tab:AddTextbox({
    Name = "Goto player",
    Default = "",
    TextDisappear = true,
    Callback = function(Value)
    PlayerName = Value
local function findPlayer(name)
    for _, player in ipairs(game.Players:GetPlayers()) do
        if player.Name:lower():match(name:lower()) or player.DisplayName:lower():match(name:lower()) then
            return player
        end
    end
    return nil
end

-- Функция для телепортации локального игрока к другому игроку
local function teleportToPlayer(targetPlayer)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
    local targetCharacter = targetPlayer.Character or targetPlayer.CharacterAdded:Wait()
    local targetHumanoidRootPart = targetCharacter:WaitForChild("HumanoidRootPart")

    humanoidRootPart.CFrame = targetHumanoidRootPart.CFrame
end

-- Пример использования: телепортируемся к игроку по его имени пользователя или отображаемому имени
local targetName = PlayerName
local targetPlayer = findPlayer(targetName)

if targetPlayer then
    teleportToPlayer(targetPlayer)
else
    print("Игрок не найден")
end
  end
})

Tab:AddButton({
    Name = "ESP",
    Callback = function()
         local function highlightPlayers()
    for _, player in ipairs(game.Players:GetPlayers()) do
        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            local highlight = Instance.new("BoxHandleAdornment")
            highlight.Name = "PlayerHighlight"
            highlight.Size = player.Character.HumanoidRootPart.Size + Vector3.new(0.2, 0.2, 0.2)
            highlight.Color3 = Color3.fromRGB(0, 255, 0) -- Можно изменить цвет подсветки
            highlight.Transparency = 0.5 -- Можно изменить прозрачность подсветки
            highlight.AlwaysOnTop = true
            highlight.Adornee = player.Character.HumanoidRootPart
            highlight.Parent = player.Character.HumanoidRootPart
            
            local label = Instance.new("BillboardGui")
            label.Name = "PlayerLabel"
            label.Size = UDim2.new(0, 100, 0, 20)
            label.StudsOffset = Vector3.new(0, 3, 0)
            label.AlwaysOnTop = true
            label.Adornee = player.Character.HumanoidRootPart
            label.Parent = player.Character.HumanoidRootPart
            
            local text = Instance.new("TextLabel")
            text.Name = "HealthLabel"
            text.Size = UDim2.new(1, 0, 1, 0)
            text.BackgroundTransparency = 1
            text.TextColor3 = Color3.new(1, 1, 1)
            text.TextStrokeTransparency = 0.5
            text.TextStrokeColor3 = Color3.new(0, 0, 0)
            text.Font = Enum.Font.SourceSansBold
            text.TextSize = 14
            text.Text = player.Name .. " - " .. player.Character.Humanoid.Health
            text.Parent = label
        end
    end
end

game.Players.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function(character)
        character:WaitForChild("HumanoidRootPart")
        highlightPlayers()
    end)
end)

game.Players.PlayerRemoving:Connect(function(player)
    local highlight = player.Character and player.Character:FindFirstChild("PlayerHighlight")
    if highlight then
        highlight:Destroy()
    end
    
    local label = player.Character and player.Character:FindFirstChild("PlayerLabel")
    if label then
        label:Destroy()
    end
end)

game:GetService("RunService").RenderStepped:Connect(function()
    for _, player in ipairs(game.Players:GetPlayers()) do
        local label = player.Character and player.Character:FindFirstChild("PlayerLabel")
        if label then
            local text = label:FindFirstChild("HealthLabel")
            if text then
                text.Text = player.Name .. " - " .. player.Character.Humanoid.Health
            end
        end
    end
end)

highlightPlayers()
    end    
})

