Tab:AddDropdown({
	Name = "Finders",
	Default = "",
	Options = {"Parts finder", "Animation finder", "Animation finder v2", "Gear name finder", "Find user id", "Find place id", "Find CFrame"},
	Callback = function(Value)
		if Value == "Parts finder" then
		local player = game.Players.LocalPlayer
if player then
    -- Пытаемся получить персонажа игрока
    local character = player.Character or player.CharacterAdded:Wait()
    if character then
        -- Получаем HumanoidRootPart (или Torso для старых моделей) как начальную точку
        local startingPart = character:FindFirstChild("HumanoidRootPart") or character:FindFirstChild("Torso")
        if startingPart then
            -- Устанавливаем targetPosition относительно позиции HumanoidRootPart
            local targetPosition = startingPart.Position + Vector3.new(-29, 1, -69) -- Смещение относительно текущего положения

            -- Теперь используем targetPosition для дальнейших действий, например, для создания Region3
            local radius = 50
            local regionSize = Vector3.new(radius * 2, radius * 2, radius * 2) -- Размер области
            local regionCenter = targetPosition
            local region = Region3.new(regionCenter - regionSize / 2, regionCenter + regionSize / 2)
            local parts = workspace:FindPartsInRegion3(region, nil, math.huge)

            for _, part in pairs(parts) do
                print("Object found:", part.Name)
                end
        else
            print("Air")
        end
    end
end

wait()

Spooky = Instance.new("Sound", game.Workspace)
Spooky.Name = "Spooky"
Spooky.SoundId = "rbxassetid://3398620867"
Spooky.Volume = 1
