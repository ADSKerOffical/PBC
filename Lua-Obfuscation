 Name = "Activate secret",
 Callback = function()
pas = true
while pas do
wait()
a = math.random(1, 10000)
if a == 10000 then
local co = Instance.new("ColorCorrectionEffect")
co.Parent = game.Lighting
co.Saturation = 150
co.TintColor = Color3.fromRGB(255, 0, 0)

Spooky = Instance.new("Sound", game.Workspace)
