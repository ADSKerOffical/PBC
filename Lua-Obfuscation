Tab:AddTextbox({
	Name = "???????",
	Default = "input",
	TextDisappear = true,
	Callback = function(Value)
	Apsw = Value
                
local password = "9928"

-- По нажатию на кнопку проверяем пароль
    if Apsw == password then
        Spooky = Instance.new("Sound", game.Workspace)
Spooky.Name = "Spooky"
Spooky.SoundId = "rbxassetid://1839246711"
Spooky.Volume = 10
Spooky.PlaybackSpeed = 1
Spooky.Looped = fasle
Spooky:Play()
    else 
        Spooky = Instance.new("Sound", game.Workspace)
Spooky.Name = "Spooky"
Spooky.SoundId = "rbxassetid://670599653"
Spooky.Volume = 1
Spooky.PlaybackSpeed = 1
Spooky.Looped = fasle
Spooky:Play()
    end
    end
})

