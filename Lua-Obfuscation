local leftLeg = character:FindFirstChild("LeftUpperLeg") 
local rightLeg = character:FindFirstChild("RightUpperLeg")

if leftLeg then
  leftLeg:Destroy()
end

if rightLeg then
  rightLeg:Destroy() 
end
		end
	end    
