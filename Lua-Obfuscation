if Value == "Find CFrame" then
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

-- Получаем CFrame локального игрока
local function getPlayerCFrame()
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
    local playerCFrame = humanoidRootPart.CFrame

    -- Получаем координаты X, Y и Z из CFrame
    local x = playerCFrame.X
    local y = playerCFrame.Y
    local z = playerCFrame.Z

    -- Выводим координаты X, Y и Z в консоль
    print("Local Player CFrame: X: " .. x .. ", Y: " .. y .. ", Z: " .. z)
    game:GetService("GuiService"):CopyToClipboard("X: " .. x .. ", Y: " .. y .. ", Z: " .. z)
end

getPlayerCFrame()

